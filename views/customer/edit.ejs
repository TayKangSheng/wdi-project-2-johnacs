<div class="container">
  <h1 class="page-header">Edit</h1>
  <h2>Customer: <%=customer.fullName%></h2>
  <h4>Devices owned: <%= customer.deviceId[0].deviceId %></h5>
  <pre><%=customer%></pre>
  <form action=<%= "/customer/" + customer.id + "?_method=PUT" %> method="POST">
    <div class="form-group">
      <ul>
        <% devices.forEach(function (device) {%>
          <li>
          <label>
          <input type="checkbox" name="device[<%=device.id%>]">
          <%= device.deviceId %>
          </label>
          </li>
          <% }) %>
        </ul>
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
  </form>
</div>
